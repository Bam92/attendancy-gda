<%- include('../includes/head.ejs')%>

<div class="container-fluid w-100 h-100 mt-5 p-5">
  <div class="row d-flex justify-content-center mb-5">
    <% presences.forEach(pres => { %>
    <div class="col-md-3 p-5">
      <div class="card bg-light">
        <div class="card-body">
          <h1 class="text-center text-danger"><%= pres.count %></h1>
          <p class="card-text text-center"><%= pres.presence %></p>
        </div>
      </div>
    </div>
    <% }) %>
  </div>

</div>
